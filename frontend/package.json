  "dependencies": {
    "@babel/core": "^7.23.5",
    "@babel/polyfill": "^7.12.1",
    "axios": "^1.6.2",
    "babel": "^6.23.0",
    "bootstrap": "^5.3.2",
    "bootstrap-vue": "^2.23.1",
    "core-js": "^3.33.3",
    "mutationobserver-shim": "^0.3.7",
    "pinia": "^2.1.7",
    "popper.js": "^1.16.1",
    "portal-vue": "^3.0.0",
    "sass": "^1.69.5",
    "sass-loader": "^13.3.2",
    "scss": "^0.2.4",
    "vue": "^3.3.4",
    "vue-cli-plugin-bootstrap-vue": "^0.8.2",
    "vue-router": "^4.2.5",
    "vue-template-compiler": "^2.7.15",
    "vuex": "^4.1.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.3",
